<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:fault="http://www.tibco.com/aff/orderservice/result" 
	xmlns:ops="http://www.tibco.com/aff/offerpriceservice" 
	xmlns:msg="http://www.tibco.com/ope/message" 
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:tns="http://www.tibco.com/aff/ope/service/soap" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	name="OfferPriceService" 
	targetNamespace="http://www.tibco.com/aff/ope/service/soap" >
	
	
	<wsdl:types>
		<xs:schema xmlns="http://www.tibco.com/aff/offerpriceservice" 
			xmlns:inv="http://www.tibco.com/AFF/classes/inventory" 
			xmlns:msg="http://www.tibco.com/ope/message" 
			xmlns:ord="http://www.tibco.com/aff/order" 
			xmlns:pr="http://www.tibco.com/ope/price" 
			xmlns:prod="http://www.tibco.com/ope/product" 
			xmlns:rs="http://www.tibco.com/aff/orderservice/result" 
			xmlns:seg="http://www.tibco.com/ope/segment" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			attributeFormDefault="unqualified" 
			elementFormDefault="qualified" 
			targetNamespace="http://www.tibco.com/aff/offerpriceservice">
			<!-- <xs:import namespace="http://www.tibco.com/AFF/classes/inventory"/> -->
			<xsd:import namespace="http://www.tibco.com/aff/order" />
			<xsd:import namespace="http://www.tibco.com/ope/segment" />
			<xsd:import namespace="http://www.tibco.com/aff/orderservice/result" />
			<xsd:import namespace="http://www.tibco.com/ope/product" />
			<xsd:import namespace="http://www.tibco.com/ope/message" />
			<xsd:import namespace="http://www.tibco.com/aff/orderservice/result" />
			<xsd:import namespace="http://www.tibco.com/ope/price" />

				<xs:complexType name="RequestControl">
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="ReturnProductInformationData">
						<xs:complexType>
							<xs:sequence>
								<xs:element maxOccurs="1" minOccurs="0" name="ReturnCharacteristicsName" type="xs:string"/>
								<xs:element maxOccurs="1" minOccurs="0" name="ReturnCharacteristicsDescription" type="xs:string"/>
								<xs:element maxOccurs="1" minOccurs="0" name="ReturnCharacteristicsValueType" type="xs:string"/>
								<xs:element maxOccurs="1" minOccurs="0" name="ReturnCharacteristicsDetails" type="xs:boolean"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="validateData" type="xs:boolean" use="required"/>
				<xs:attribute name="validateProdDate" type="xs:boolean"/>
				<xs:attribute name="validateProductRequiredForGroups" type="xs:boolean"/>
				<xs:attribute name="validateProductComprisedOfGroups" type="xs:boolean"/>
				<xs:attribute name="validateProductCompatibility" type="xs:boolean"/>
				<xs:attribute name="validateSegmentCompatibility" type="xs:boolean"/>
				<xs:attribute name="enforceCompatibleSegment" type="xs:boolean"/>
				<xs:attribute name="skipChildFilterValidation" type="xs:boolean"/>
				<xs:attribute name="decomposeProducts" type="xs:boolean"/>
				<xs:attribute name="basicValidationOnExistingOffer" type="xs:boolean"/>
			</xs:complexType>
			
			
			
			
			
			<xs:element name="ValidateOfferRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="RequestControl" type="RequestControl"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="seg:Segment"/>
						<xs:element ref="ord:orderRequest"/>
						<!-- <xs:element ref="inv:InventoryItem" minOccurs="0" maxOccurs="unbounded"/> -->
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
					<xs:attribute name="CalculateConsequences" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			
			
			<xs:element name="ValidateOfferResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="rs:resultStatus"/>
						<xs:element name="ValidationPass" type="xs:boolean"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ValidationResultMessage">
							<xs:complexType>
								<xs:sequence>
									<xs:choice>
										<xs:element name="Linenumber" type="xs:string"/>
										<xs:element name="InventoryID" type="xs:string"/>
									</xs:choice>
									<xs:element name="ProductID" type="xs:string"/>
									<xs:element maxOccurs="unbounded" ref="msg:Message"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" ref="ord:orderRequest"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			
		<xs:complexType name="Focus">
				<xs:choice>
					<xs:element name="LineNumber" type="xs:string"/>
					<xs:element name="InventoryID" type="xs:string"/>
				</xs:choice>
			</xs:complexType>
			<xs:complexType name="Field">
				<xs:sequence>
					<xs:element name="Name" type="xs:string"/>
					<xs:element name="Value" type="xs:string"/>
				</xs:sequence>
				<xs:attribute name="isFilter" type="xs:boolean" use="required"/>
			</xs:complexType>
			<xs:attribute name="BusinessTransactionID" type="xs:string"/>
			
			
			<xs:element name="GetOfferRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="RequestControl" type="RequestControl"/>
						<xs:element minOccurs="0" name="SubscriberID" type="xs:string"/>
						<xs:element minOccurs="0" name="CustomerID" type="xs:string"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="seg:Segment"/>
						<xs:element minOccurs="0" name="OrderDate" type="xs:dateTime"/>
						<xs:element minOccurs="0" name="Status" type="xs:string"/>
						<xs:element minOccurs="0" name="RecordTypes">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" name="RecordType" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="Promotions">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" name="ProductID" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="RecordSubType">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" name="RecordSubType" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="Categories">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" name="Category" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="Focus" type="Focus"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="InputFields" type="Field"/>
						<xs:element minOccurs="0" ref="ord:orderRequest"/>
						<!-- <xs:element name="Inventory" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="inv:InventoryItem" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element> -->
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
					<xs:attribute name="ReturnIneligibleProducts" type="xs:boolean"/>
					<xs:attribute name="ReturnBundleOfferings" type="xs:boolean"/>
					<xs:attribute name="ReturnPrices" type="xs:boolean"/>
					<xs:attribute name="ReturnProductInformation" type="xs:boolean"/>
					<xs:attribute name="SortFields" type="xs:string"/>
					<xs:attribute name="SortOrder" type="xs:string"/>
					<xs:attribute name="PageSize" type="xs:int"/>
					<xs:attribute name="PageNumber" type="xs:int"/>
				</xs:complexType>
			</xs:element>
			
			
			<xs:element name="GetOfferResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" ref="rs:resultStatus"/>
						<xs:element minOccurs="0" name="EligibleProducts">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="EligibleProduct">
										<xs:complexType>
											<xs:sequence>
												<xs:element maxOccurs="1" minOccurs="0" ref="prod:ProductInformation"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="ord:udf"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="msg:Message"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="pr:PriceItem"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element minOccurs="0" name="IneligibleProducts">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="InEligibleProduct">
										<xs:complexType>
											<xs:sequence>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="prod:ProductInformation"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="ord:udf"/>
												<xs:element maxOccurs="unbounded" minOccurs="0" ref="msg:Message"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			
			
			<xs:element name="GetPricesResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="pr:responses"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="GetPricesRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="pr:requests"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
					<xs:attribute name="ReturnTotals" type="xs:boolean"/>
				</xs:complexType>
			</xs:element>
			
			
			
			<xs:element name="GetProductInformationRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:choice minOccurs="0">
							<xs:element ref="prod:Query"/>
							<xs:element ref="prod:IDList"/>
						</xs:choice>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="GetProductInformationResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" ref="rs:resultStatus"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="prod:ProductInformation"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			
			
			<xs:element name="GetPriceInformationRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="pr:PriceInformationRequestBody"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="GetPriceInformationResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="pr:PriceInformationResponseBody"/>
					</xs:sequence>
					<xs:attribute name="BusinessTransactionID" type="xs:string"/>
				</xs:complexType>
			</xs:element>
		</xs:schema>
		
		<xs:schema xmlns="http://www.tibco.com/ope/product" 
			xmlns:msg="http://www.tibco.com/ope/message" 
			xmlns:ord="http://www.tibco.com/aff/order" 
			xmlns:seg="http://www.tibco.com/ope/segment" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			attributeFormDefault="unqualified" 
			elementFormDefault="qualified" targetNamespace="http://www.tibco.com/ope/product">

			<xsd:import namespace="http://www.tibco.com/aff/order" />
			<xsd:import namespace="http://www.tibco.com/ope/segment" />


				<xs:element name="ProductInformation">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="1" ref="Product"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="Characteristic"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="ItemSpecs"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="ord:udf"/>
					</xs:sequence>
					<xs:attribute name="ProductCount" type="xs:long" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Product">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="1" name="ProductID" type="xs:string"/>
						<xs:element maxOccurs="1" minOccurs="0" name="InternalID" type="xs:string"/>
						<xs:element maxOccurs="1" minOccurs="0" name="ParentInternalID" type="xs:string"/>
						<xs:element maxOccurs="1" minOccurs="0" name="SubscriberID" type="xs:string"/>
						<xs:element maxOccurs="1" minOccurs="0" name="CustomerID" type="xs:string"/>
						<xs:element maxOccurs="1" minOccurs="0" name="InventoryID" type="xs:string"/>
						<xs:element minOccurs="0" name="ProductName" type="xs:string"/>
						<xs:element minOccurs="0" name="ProductType" type="xs:string"/>
						<xs:element minOccurs="0" name="ProductSubType" type="xs:string"/>
						<xs:element minOccurs="0" name="Description" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Characteristic">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Name" type="xs:string"/>
						<xs:element minOccurs="0" name="Description" type="xs:string"/>
						<xs:element minOccurs="0" name="InstanceOptional" type="xs:string"/>
						<xs:element minOccurs="0" name="InstanceMin" type="xs:string"/>
						<xs:element minOccurs="0" name="InstanceMax" type="xs:string"/>
						<xs:element minOccurs="0" name="instanceCeaseSequence" type="xs:string"/>
						<xs:element minOccurs="0" name="instanceUpdateSequence" type="xs:string"/>
						<xs:element minOccurs="0" name="instanceSequence" type="xs:string"/>
						<xs:element minOccurs="0" name="Values">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="Value">
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="ValueName" type="xs:string"/>
												<xs:element minOccurs="0" name="ValueType" type="xs:string"/>
												<xs:element minOccurs="0" name="Value" type="xs:string"/>
												<xs:element minOccurs="0" name="ValueFrom" type="xs:string"/>
												<xs:element minOccurs="0" name="ValueTo" type="xs:string"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ItemSpecs">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Product"/>
						<xs:element name="SingleUse" type="xs:boolean"/>
						<xs:element name="MustComplete" type="xs:boolean"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="Characteristic"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Query">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Class" type="xs:string"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="SubClass" type="xs:string"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Category" type="xs:string"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="seg:Segment"/>
						<!-- <xs:element name="onlyActiveProducts" type="xs:boolean"/> -->
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="IDList">
				<xs:complexType>
					<xs:sequence>
						<!-- <xs:element name="ID" type="xs:string" minOccurs="0" maxOccurs="unbounded"/> -->
						<xs:element maxOccurs="unbounded" ref="IDs"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="IDs">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="0" name="ID" type="xs:string"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="ord:udf"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

		</xs:schema>
		
		<xs:schema xmlns="http://www.tibco.com/aff/order" 
			xmlns:types="http://www.tibco.com/aff/commontypes" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.tibco.com/aff/order" version="1.1">
			<xsd:import namespace="http://www.tibco.com/aff/commontypes" />


			<!-- xs:element name="orderType"> -->
			<xs:complexType name="orderType">
				<xs:sequence>
					<xs:element name="orderID">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="orderRef">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="header" type="headerType" />
					<xs:element maxOccurs="unbounded" minOccurs="0" name="line" type="lineType" />
					<xs:element minOccurs="0" name="request">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="orderRequest" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element minOccurs="0" name="amendments">
						<xs:complexType>
							<xs:sequence>
								<xs:element maxOccurs="unbounded" ref="orderAmendment" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element minOccurs="0" name="extension" type="types:extensionType" />
				</xs:sequence>
			</xs:complexType>
			<!-- </xs:element> -->
			<xs:complexType name="orderRequestType">
				<xs:sequence>
					<xs:element name="orderRef">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="125" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="header" type="headerRequestType" />
					<xs:element maxOccurs="unbounded" name="line" type="lineRequestType" />
					<xs:element minOccurs="0" name="extension" type="types:extensionType" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="headerType">
				<xs:sequence>
					<xs:element name="submittedDate" type="xs:dateTime" />
					<xs:element minOccurs="0" name="description">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element minOccurs="0" name="customerID">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="64" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element minOccurs="0" name="subscriberID">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="64" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:choice minOccurs="0">
						<xs:element minOccurs="0" name="requiredByDate" type="xs:dateTime" />
						<xs:element name="requiredOnDate" type="xs:dateTime" />
					</xs:choice>
					<xs:element default="4" minOccurs="0" name="orderPriority">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0" />
								<xs:maxInclusive value="9" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element minOccurs="0" name="invoiceAddress" type="types:addressType" />
					<xs:element minOccurs="0" name="deliveryAddress" type="types:addressType" />
					<xs:element minOccurs="0" name="notes">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="slaID">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="32" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" ref="udf" />
					<xs:element name="pointOfNoReturn" type="xs:boolean" />
					<xs:element name="status">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="22" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="statusChanged" type="xs:dateTime" />
					<xs:element name="suspended" type="xs:boolean" />
					<xs:element name="originator">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="32" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element minOccurs="0" name="planUniqueID" type="xs:string" />
					<xs:element minOccurs="0" name="planStatus" type="xs:string" />
					<xs:element minOccurs="0" name="planExpectedCompletionDate" type="xs:dateTime" />
					<xs:element minOccurs="0" name="warning" type="xs:string" />
					<xs:element minOccurs="0" name="extension" type="types:extensionType" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="headerRequestType">
				<xs:sequence>
					<xs:element minOccurs="0" name="description">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:choice>
						<xs:element name="customerID">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="64" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="subscriberID">
							<xs:simpleType>
